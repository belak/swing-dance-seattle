{
  "extends": ["formidable/configurations/es6-react"],
  "rules": {
    "consistent-return": 0,
    "indent": ["error", 2, { "SwitchCase": 1 }],
    "max-len": ["error", { "code": 80 }],
    "no-invalid-this": 0,
    "no-unused-vars": ["error", { "ignoreRestSiblings": true }],
    "react/no-multi-comp": 0
  }
}
